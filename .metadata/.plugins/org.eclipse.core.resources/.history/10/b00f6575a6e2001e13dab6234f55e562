	public Student(EmailService emailService, JavaCourseService javaCourseService) {
		super();
		this.emailService = emailService;
		this.javaCourseService = javaCourseService;
	}
	public void details()
	{
		emailService.message();
		javaCourseService.courseInfo();
	}
	  
	  
}
